{
  "manifest_version": 3,
  "name": "Morning Quotes - Fast and Simple",
  "description": "Start Your Day With A Quote, Provided By DrPanayioths",
  "version": "1.0",
  "action": {
    "default_popup": "main.html"
  },
  "permissions": ["tabs", "storage"],
  "icons": {
    "32": "favicon.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
